{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "Simple app written in Typescript that allows users to shorten their url",
  "main": "index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "start-dev": "env-cmd -f ./config/dev.env nodemon dist/index.js",
    "build": "webpack --mode production --config webpack.web.config.js && webpack --mode production --config webpack.server.config.js",
    "build-dev": "NODE_ENV=development concurrently \"webpack --mode development --config webpack.web.config.js\" \"webpack --mode development --config webpack.server.config.js\"",
    "test": "env-cmd -f ./config/test.env jest"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "env-cmd": "^10.1.0",
    "express": "^4.16.4",
    "mongoose": "^5.13.8",
    "morgan": "~1.9.1"
  },
  "author": "Konrad Dobosz <kdobosz02@gmail.com> (https://kdobosz.pl/)",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^15.14.9",
    "@types/supertest": "^2.0.11",
    "concurrently": "^6.2.1",
    "css-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.2",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.1.0",
    "mini-css-extract-plugin": "^2.2.0",
    "nodemon": "^2.0.12",
    "prettier": "2.3.1",
    "sass": "^1.38.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.2.2"
  }
}
